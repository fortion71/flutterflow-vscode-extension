{
  "name": "flutterflow-code-export",
  "displayName": "FlutterFlow Code Export",
  "description": "Allows to easily manage code exports for your flutterflow projects.",
  "icon": "images/icon.png",
  "publisher": "krabhishek",
  "repository": {
    "type": "git",
    "url": "https://github.com/krabhishek/flutterflow-vscode-extension.git"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flutterflow-code.sync"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutterflow-code.sync",
        "title": "Download flutterflow project code."
      }
    ],
    "keybindings": [
      {
        "command": "flutterflow-code.sync",
        "key": "ctrl+alt+shift+f",
        "mac": "cmd+ctrl+shift+f"
      }
    ],
    "configuration": {
      "title": "FlutterFlow",
      "properties": {
        "flutterflow.userApiToken": {
          "type": "string",
          "description": "Your FlutterFlow API Token. You can find it on your API token in your flutteflow account page."
        },
        "flutterflow.activeProject": {
          "type": "string",
          "description": "Your flutterflow project you want to sync."
        },
        "flutterflow.workingDirectory": {
          "type": "string",
          "description": "Full path of directory where you want the code to be saved. Usually the folder that is open in VScode."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}
